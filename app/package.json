{
  "name": "wh40k10ed-api",
  "version": "1.0.0",
  "description": "API server and data pipeline for Warhammer 40k data.",
  "main": "dist/api/server.js",
  "scripts": {
    "start": "node dist/api/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/api/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "script:updateDB": "ts-node src/scripts/updateDB.ts",
    "script:populateDB": "ts-node src/scripts/populateDB.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "better-sqlite3": "^11.0.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "papaparse": "^5.5.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "node",
    "typescript",
    "express"
  ],
  "author": "AEMuto",
  "license": "ISC"
}
